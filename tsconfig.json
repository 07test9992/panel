{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "strict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "lib": [
      "es2016",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./resources/assets/scripts/*"
      ]
    }
  },
  "include": [
    "./resources/assets/scripts/**/*"
  ]
}
